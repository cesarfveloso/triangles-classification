{
  "name": "triangle-classification",
  "version": "1.0.0",
  "description": "Determining a triangle classification",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:cesarfveloso/triangles-classification.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "nodemon -r dotenv/config src/index.ts",
    "start:debug": "nodemon --exec 'node --inspect -r dotenv/config --require ts-node/register src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:e2e": "jest --projects ./test --runInBand",
    "test:unit": "export DOTENV_CONFIG_PATH=test.env && jest"
  },
  "keywords": [
    "triangle",
    "triangle-classification",
    "equilateral",
    "isosceles",
    "scalene"
  ],
  "author": "Cesar Veloso",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "aws-cdk-lib": "^2.3.0",
    "aws-sdk": "^2.1048.0",
    "body-parser": "^1.19.1",
    "class-validator": "^0.13.2",
    "constructs": "^10.0.16",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dynamodb-local": "0.0.31",
    "express": "^4.17.2",
    "express-rate-limit": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.45.0",
    "@shelf/jest-dynamodb": "^2.2.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.3",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "aws-cdk": "^2.3.0",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
