{
  "name": "triangle-classification",
  "version": "1.0.0",
  "description": "Determining a triangle classification",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:cesarfveloso/triangles-classification.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "start:dev": "export NODE_ENV=development && nodemon src/index.ts",
    "start:debug": "export NODE_ENV=development && nodemon --exec 'node --inspect --require ts-node/register src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest --projects ./test --runInBand",
    "test:unit": "jest"
  },
  "keywords": [
    "triangle",
    "triangle-classification",
    "equilateral",
    "isosceles",
    "scalene"
  ],
  "author": "Cesar Veloso",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "aws-cdk-lib": "^2.3.0",
    "aws-sdk": "^2.1048.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "class-validator": "^0.13.2",
    "config": "^3.3.6",
    "constructs": "^10.0.16",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-http-context": "^1.2.4",
    "express-rate-limit": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^3.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.3",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "aws-cdk": "^2.3.0",
    "dynamodb-local": "0.0.31",
    "eslint": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}